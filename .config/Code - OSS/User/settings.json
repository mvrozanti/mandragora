{
  "editor.tokenColorCustomizations": {
    "functions": "#c72900",
    "keywords": "#e33700",
    "numbers": "#ee3800",
    "strings": "#ff611f",
    "types": "#ff6c30",
    "variables": "#ff7d1e",
    "comments": "#a28473",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#e33700"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#ee3800"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#191420",
    "editorBracketHighlight.foreground1": "#c72900",
    "editorBracketHighlight.foreground2": "#e33700",
    "editorBracketHighlight.foreground3": "#ee3800",
    "editorBracketHighlight.foreground4": "#ff611f",
    "editorBracketHighlight.foreground5": "#ff6c30",
    "editorBracketHighlight.foreground6": "#ff7d1e",
    "editorBracketHighlight.unexpectedBracket.foreground": "#a28473",
    "tab.activeBorderTop": "#c72900"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "editor.detectIndentation": false,
  "vscode-openai.serviceProvider": "Custom-OpenAI",
  "vscode-openai.baseUrl": "",
  "vscode-openai.defaultModel": "",
  "vscode-openai.azureDeployment": "setup-required",
  "vscode-openai.scmModel": "",
  "vscode-openai.scmDeployment": "setup-required",
  "vscode-openai.embeddingModel": "setup-required",
  "vscode-openai.embeddingsDeployment": "setup-required",
  "vscode-openai.azureApiVersion": "2023-05-15",
  "vscode-openai.conversation-configuration.colors": {
    "lightUserColor": "#424242",
    "lightUserBackground": "#f0f0f0",
    "lightAssistantColor": "#107c10",
    "lightAssistantBackground": "#f1faf1",
    "darkUserColor": "#d6d6d6",
    "darkUserBackground": "#0a0a0a",
    "darkAssistantColor": "#9fd89f",
    "darkAssistantBackground": "#052505"
  },
  "glassit.alpha": 220,
  "python.createEnvironment.trigger": "off",
  "flake8.severity": {
    "E": "Warning",
    "F": "Warning"
  }
}
