{
  "editor.tokenColorCustomizations": {
    "functions": "#6B5253",
    "keywords": "#655D62",
    "numbers": "#6F6B6B",
    "strings": "#BA3A3D",
    "types": "#966162",
    "variables": "#957D82",
    "comments": "#8c8787",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#655D62"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#6F6B6B"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#090808",
    "editorBracketHighlight.foreground1": "#6B5253",
    "editorBracketHighlight.foreground2": "#655D62",
    "editorBracketHighlight.foreground3": "#6F6B6B",
    "editorBracketHighlight.foreground4": "#BA3A3D",
    "editorBracketHighlight.foreground5": "#966162",
    "editorBracketHighlight.foreground6": "#957D82",
    "editorBracketHighlight.unexpectedBracket.foreground": "#8c8787",
    "tab.activeBorderTop": "#6B5253"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "glassit.alpha": 255,
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "pylint.importStrategy": "fromEnvironment",
  "python.languageServer": "Jedi",
  "python.defaultInterpreterPath": "/usr/bin/python",
  "pylint.interpreter": [
    "/usr/bin/python"
  ],
  "pylint.path": [
    "~/.env/python",
    "-m",
    "pylint"
  ]
}
