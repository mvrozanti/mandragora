{
  "editor.tokenColorCustomizations": {
    "functions": "#C9AD30",
    "keywords": "#DDA05D",
    "numbers": "#F0E564",
    "strings": "#B15B8F",
    "types": "#D5A798",
    "variables": "#F8F0A0",
    "comments": "#abaa9c",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#DDA05D"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#F0E564"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#200305",
    "editorBracketHighlight.foreground1": "#C9AD30",
    "editorBracketHighlight.foreground2": "#DDA05D",
    "editorBracketHighlight.foreground3": "#F0E564",
    "editorBracketHighlight.foreground4": "#B15B8F",
    "editorBracketHighlight.foreground5": "#D5A798",
    "editorBracketHighlight.foreground6": "#F8F0A0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#abaa9c",
    "tab.activeBorderTop": "#C9AD30"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "glassit.alpha": 255,
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "pylint.importStrategy": "fromEnvironment",
  "python.defaultInterpreterPath": "/usr/bin/python",
  "python.languageServer": "Jedi",
  "window.zoomLevel": 2,
  "editor.fontLigatures": false,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on"
}
