{
  "editor.tokenColorCustomizations": {
    "functions": "#BB414E",
    "keywords": "#FB9E5D",
    "numbers": "#F1D354",
    "strings": "#563293",
    "types": "#52409C",
    "variables": "#AC47A0",
    "comments": "#71859d",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#FB9E5D"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#F1D354"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#050103",
    "editorBracketHighlight.foreground1": "#BB414E",
    "editorBracketHighlight.foreground2": "#FB9E5D",
    "editorBracketHighlight.foreground3": "#F1D354",
    "editorBracketHighlight.foreground4": "#563293",
    "editorBracketHighlight.foreground5": "#52409C",
    "editorBracketHighlight.foreground6": "#AC47A0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#71859d",
    "tab.activeBorderTop": "#BB414E"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ]
}
