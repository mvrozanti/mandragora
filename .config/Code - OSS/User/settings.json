{
  "editor.tokenColorCustomizations": {
    "functions": "#EA9768",
    "keywords": "#63589F",
    "numbers": "#4855AF",
    "strings": "#996D98",
    "types": "#6A90DD",
    "variables": "#DEA697",
    "comments": "#989193",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#63589F"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#4855AF"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#0A0308",
    "editorBracketHighlight.foreground1": "#EA9768",
    "editorBracketHighlight.foreground2": "#63589F",
    "editorBracketHighlight.foreground3": "#4855AF",
    "editorBracketHighlight.foreground4": "#996D98",
    "editorBracketHighlight.foreground5": "#6A90DD",
    "editorBracketHighlight.foreground6": "#DEA697",
    "editorBracketHighlight.unexpectedBracket.foreground": "#989193",
    "tab.activeBorderTop": "#EA9768"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "editor.detectIndentation": false
}
