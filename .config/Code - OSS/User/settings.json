{
  "editor.tokenColorCustomizations": {
    "functions": "#764957",
    "keywords": "#8A3449",
    "numbers": "#A9374F",
    "strings": "#9F5764",
    "types": "#B85263",
    "variables": "#C57A84",
    "comments": "#9e8989",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#8A3449"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#A9374F"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#100111",
    "editorBracketHighlight.foreground1": "#764957",
    "editorBracketHighlight.foreground2": "#8A3449",
    "editorBracketHighlight.foreground3": "#A9374F",
    "editorBracketHighlight.foreground4": "#9F5764",
    "editorBracketHighlight.foreground5": "#B85263",
    "editorBracketHighlight.foreground6": "#C57A84",
    "editorBracketHighlight.unexpectedBracket.foreground": "#9e8989",
    "tab.activeBorderTop": "#764957"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ]
}
