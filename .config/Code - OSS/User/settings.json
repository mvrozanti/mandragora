{
  "editor.tokenColorCustomizations": {
    "functions": "#A2986E",
    "keywords": "#C4AA71",
    "numbers": "#918E89",
    "strings": "#A8A88D",
    "types": "#D0AF92",
    "variables": "#B0CC9B",
    "comments": "#9f9c8c",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#C4AA71"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#918E89"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#130f15",
    "editorBracketHighlight.foreground1": "#A2986E",
    "editorBracketHighlight.foreground2": "#C4AA71",
    "editorBracketHighlight.foreground3": "#918E89",
    "editorBracketHighlight.foreground4": "#A8A88D",
    "editorBracketHighlight.foreground5": "#D0AF92",
    "editorBracketHighlight.foreground6": "#B0CC9B",
    "editorBracketHighlight.unexpectedBracket.foreground": "#9f9c8c",
    "tab.activeBorderTop": "#A2986E"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "editor.detectIndentation": false
}
