{
  "editor.tokenColorCustomizations": {
    "functions": "#AB4D02",
    "keywords": "#D64E01",
    "numbers": "#DD6302",
    "strings": "#E77003",
    "types": "#DE880E",
    "variables": "#F59308",
    "comments": "#ac8f56",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#D64E01"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#DD6302"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#0C0C0C",
    "editorBracketHighlight.foreground1": "#AB4D02",
    "editorBracketHighlight.foreground2": "#D64E01",
    "editorBracketHighlight.foreground3": "#DD6302",
    "editorBracketHighlight.foreground4": "#E77003",
    "editorBracketHighlight.foreground5": "#DE880E",
    "editorBracketHighlight.foreground6": "#F59308",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ac8f56",
    "tab.activeBorderTop": "#AB4D02"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "editor.detectIndentation": false
}
