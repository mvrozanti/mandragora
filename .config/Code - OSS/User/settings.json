{
  "editor.tokenColorCustomizations": {
    "functions": "#678F2A",
    "keywords": "#719156",
    "numbers": "#9FAA38",
    "strings": "#95995D",
    "types": "#738E92",
    "variables": "#63B4AC",
    "comments": "#868d8d",
    "textMateRules": [
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#719156"
        }
      },
      {
        "scope": "keyword.operator.logical.python",
        "settings": {
          "foreground": "#9FAA38"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "workbench.colorCustomizations": {
    "editor.background": "#151910",
    "editorBracketHighlight.foreground1": "#678F2A",
    "editorBracketHighlight.foreground2": "#719156",
    "editorBracketHighlight.foreground3": "#9FAA38",
    "editorBracketHighlight.foreground4": "#95995D",
    "editorBracketHighlight.foreground5": "#738E92",
    "editorBracketHighlight.foreground6": "#63B4AC",
    "editorBracketHighlight.unexpectedBracket.foreground": "#868d8d",
    "tab.activeBorderTop": "#678F2A"
  },
  "keyboard.dispatch": "keyCode",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>",
        "jk"
      ]
    }
  ],
  "razor.disabled": true,
  "files.autoSave": "afterDelay",
  "omnisharp.useGlobalMono": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "window.autoDetectColorScheme": true,
  "editor.bracketPairColorization.enabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "'Monocraft Nerd Font'",
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {},
  "window.zoomLevel": 1,
  "editor.fontLigatures": false,
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "flake8.interpreter": [
    "python3"
  ],
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "editor.detectIndentation": false
}
