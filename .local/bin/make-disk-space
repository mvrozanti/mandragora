#!/bin/bash
yes | sudo journalctl --vacuum-size=1G 
yes | sudo rm -r /var/lib/systemd/coredump/core.* 
yes | sudo flatpak uninstall --unused
yes | trash-empty
yes | sudo rm -rf /var/lib/snapd/cache/*
# yes | sudo rm -rf /var/cache/pacman/pkg/download-*
yes | docker system prune -a --volumes
yes | pip cache purge
yes | yarn cache clean
if [[ ! -z `pacman -Qtdq` ]]; then
    yes | sudo pacman -Rns $(pacman -Qtdq)
else
    echo 'No orphan packages.'
fi
go clean -cache
cargo cache -a
conda clean -a -y 
yes | trash-empty
yes | sudo pacman -Scc
yes | yay -Scc 
npm cache clean --force
rm ~/.xsession-errors* || true
rm -rf /home/m/.gradle/caches
rm -rf /home/m/.cache/ueberzugpp
flatpak uninstall --delete-data
rm -rf /home/m/.config/discord/Cache
rm -rf /home/m/.config/whatsappweb-nativefier-d40211/Cache
rm -rf /home/m/.config/whatsappweb-nativefier-d40211/Code Cache
rm -rf /home/m/.config/Code - OSS/Cache
rm -rf /home/m/.config/Code/Cache
