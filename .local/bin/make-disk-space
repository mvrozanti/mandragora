#!/bin/bash
rm ~/.xsession-errors*
yes | yay -Scc 
yes | trash-empty
yes | sudo journalctl --vacuum-size=1G 
yes | sudo rm -r /var/lib/systemd/coredump/core.* 
yes | sudo pacman -Scc
yes | docker system prune -a
yes | pip cache purge
yes | yarn cache clean
yes | sudo pacman -Rns $(pacman -Qtdq)
yes | trash-empty
